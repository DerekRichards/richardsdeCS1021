<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane fx:id="mainGrid" alignment="center" hgap="10" vgap="10" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1" fx:controller="sohoni.MainController">
    <children>
    <VBox prefHeight="200.0" prefWidth="100.0">
        <children>
            <Button fx:id="open" defaultButton="true" maxWidth="80.0" minWidth="80.0" mnemonicParsing="false" onAction="#open" text="Open" />
            <Button fx:id="close" cancelButton="true" maxWidth="80.0" minWidth="80.0" mnemonicParsing="false" onAction="#close" text="Close" />
            <Button fx:id="sayHi" mnemonicParsing="false" onAction="#sayHi" text="say Hi" />
            <Label fx:id="label" text="Label" />
        </children>
    </VBox>
    </children>
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
</GridPane>
