<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="richardsde.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <HBox prefHeight="259.0" prefWidth="295.0">
         <children>
            <VBox prefHeight="200.0" prefWidth="100.0">
               <children>
                  <ImageView fx:id="displayedImage" fitHeight="259.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
               </children></VBox>
            <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="109.0" spacing="5.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#load" prefHeight="25.0" prefWidth="85.0" text="Open" />
                  <Button mnemonicParsing="false" onAction="#save" prefHeight="25.0" prefWidth="85.0" text="Save" />
                  <Button mnemonicParsing="false" onAction="#reload" prefHeight="25.0" prefWidth="86.0" text="Reload" />
                  <Button mnemonicParsing="false" onAction="#grayscale" prefHeight="25.0" prefWidth="89.0" text="Grayscale" />
                  <Button mnemonicParsing="false" onAction="#negative" prefHeight="25.0" prefWidth="85.0" text="Negative" />
               </children></VBox>
         </children>
      </HBox>
   </children>
</GridPane>
